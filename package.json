    "@stitches/react": "^1.2.8",
    "esbuild": "^0.14.38",
    "mdx-bundler": "^9.0.0",
    "motion": "^10.8.1",
    "next": "12.1.6",
    "next-themes": "^0.1.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "remark-autolink-headings": "^7.0.1",
    "remark-mdx-images": "^1.0.3",
    "remark-slug": "^7.0.1",
    "stitches-reset": "^1.0.0",
    "styled-icons": "^10.45.0"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "typescript": "4.6.2"
  }
