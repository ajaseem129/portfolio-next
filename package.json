{
  "name": "portfolio-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "check-types": "tsc --noEmit",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx|md|mdx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm run check-types && npm run check-format && npm run lint",
    "postinstall": "npx husky install"
  },
  "dependencies": {
    "@fontsource/bree-serif": "^4.5.3",
    "@fontsource/ibm-plex-mono": "^4.5.4",
    "@fontsource/ibm-plex-sans": "^4.5.5",
    "@headlessui/react": "^1.5.0",
    "@stitches/react": "^1.2.7",
    "motion": "^10.7.0",
    "next": "12.1.0",
    "next-themes": "^0.1.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "stitches-reset": "^1.0.0",
    "styled-icons": "^10.45.0"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "typescript": "4.6.2"
  }
}
